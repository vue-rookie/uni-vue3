{
  "globalStyle": {
    "navigationStyle": "default",
    "navigationBarTitleText": "uni-vue3",
    "navigationBarBackgroundColor": "#f8f8f8",
    "navigationBarTextStyle": "black",
    "backgroundColor": "#FFFFFF"
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uve-(.*)": "@/components/uve-$1/index.vue"
    }
  },
  "tabBar": {
    "color": "#8f9ca2",
    "selectedColor": "#ff2442",
    "backgroundColor": "#ffffff",
    "borderStyle": "black",
    "height": "64px",
    "fontSize": "14px",
    "iconWidth": "32px",
    "spacing": "0px",
    "list": [
      {
        "iconPath": "static/tabbar/home.png",
        "selectedIconPath": "static/tabbar/home-select.png",
        "pagePath": "pages/xiaohongshu/index",
        "text": "首页"
      },
      {
        "iconPath": "static/tabbar/tab1.png",
        "selectedIconPath": "static/tabbar/tab1-select.png",
        "pagePath": "pages/xiaohongshu/profile",
        "text": "我"
      }
    ]
  },
  "pages": [
    {
      "path": "pages/xiaohongshu/index",
      "type": "home",
      "style": {
        "navigationBarTitleText": "小红书",
        "navigationStyle": "custom"
      },
      "needLogin": false
    },
    {
      "path": "pages/xiaohongshu/profile",
