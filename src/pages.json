{
  "globalStyle": {
    "navigationStyle": "default",
    "navigationBarTitleText": "uni-vue3",
    "navigationBarBackgroundColor": "#f8f8f8",
    "navigationBarTextStyle": "black",
    "backgroundColor": "#FFFFFF"
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uve-(.*)": "@/components/uve-$1/index.vue"
    }
  },
  "tabBar": {
    "color": "#8f9ca2",
    "selectedColor": "#000",
    "backgroundColor": "#ffffff",
    "borderStyle": "black",
    "height": "64px",
    "fontSize": "14px",
    "iconWidth": "32px",
    "spacing": "0px",
    "list": [
      {
        "iconPath": "static/tabbar/home.png",
        "selectedIconPath": "static/tabbar/home-select.png",
        "pagePath": "pages/index/index",
        "text": "页面"
      },
      {
        "iconPath": "static/tabbar/tab1.png",
        "selectedIconPath": "static/tabbar/tab1-select.png",
        "pagePath": "pages/use/index",
        "text": "方法"
      }
    ]
  },
  "pages": [
    {
      "path": "pages/index/index",
      "type": "home",
      "style": {
        "navigationBarTitleText": "首页",
        "navigationStyle": "default"
      },
      "needLogin": false
    },
    {
      "path": "pages/user/index",
      "type": "page",
      "style": {
        "navigationBarTitleText": "用户中心",
        "navigationStyle": "default"
      },
      "needLogin": false
    },
    {
      "path": "pages/use/index",
      "type": "page",
      "style": {
        "navigationBarTitleText": "方法示例",
        "navigationStyle": "default"
      },
      "needLogin": false
    },
    {
      "path": "pages/settings/index",
      "type": "page",
      "style": {
        "navigationBarTitleText": "设置",
        "navigationStyle": "default"
      },
      "needLogin": false
    },
    {
      "path": "pages/login/index",
      "type": "page",
      "style": {
        "navigationBarTitleText": "登录注册",
        "navigationStyle": "default"
      },
      "needLogin": false
    }
  ],
  "subPackages": [
    {
      "root": "pages-sub",
      "pages": [
        {
          "path": "button-demo/index",
          "type": "page",
          "style": {
            "navigationBarTitleText": "uve-button按钮",
            "navigationStyle": "default"
          }
        },
        {
          "path": "border-demo/index",
          "type": "page",
          "style": {
            "navigationBarTitleText": "uve-border边框",
            "navigationStyle": "default"
          }
        },
        {
          "path": "picker-demo/index",
          "type": "page",
          "style": {
            "navigationBarTitleText": "uve-picker选择器",
            "navigationStyle": "default"
          }
        },
        {
          "path": "message-demo/index",
          "type": "page",
          "style": {
            "navigationBarTitleText": "uve-message消息",
            "navigationStyle": "default"
          }
        },
        {
          "path": "tabs-demo/index",
          "type": "page",
          "style": {
            "navigationBarTitleText": "uve-tabs标签",
            "navigationStyle": "default"
          }
        }
      ]
    }
  ]
}